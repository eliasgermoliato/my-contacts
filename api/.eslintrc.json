{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-console": 0,
        "class-methods-use-this": 0,
        "import/no-extraneous-dependencies": 0,
        "consistent-return": 0,
        "camelcase": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
